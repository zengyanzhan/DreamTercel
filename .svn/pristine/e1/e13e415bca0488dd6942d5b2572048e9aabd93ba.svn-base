	/**
	 * 根据登录的用户查询权限
	 * @return 视图对象
	 * @throws Exception 异常
	 */
	@RequestMapping(value="/loadRightforMain.action")
	public ModelAndView loadRightforMain(HttpServletRequest request) throws Exception{
		//创建 会话对象
		HttpSession session =request.getSession(false);
		if(session!=null){
			request.getSession(true);
		}
		//得到用户对象
		User user = (User)session.getAttribute("user");
		//得到标志
		Integer flag=user.getUserFlag();
		if(flag!=null&&flag==-1){//系统的初始状态
			//查询父功能
			List<Right> documenRighttList=rightService.selectSysRightByDocument(user.getUserName());
			//创建有序集合
			HashMap<Right,List<Right>> rightMap = new LinkedHashMap<Right, List<Right>>();;
			//循环父功能集合
			for (int i = 0; i < documenRighttList.size(); i++) {
				//得到每个父功能
				Right right = documenRighttList.get(i);
				//判断是否为系统初始状态
				if(right.getRightText().equals("系统管理")||right.getRightText().equals("业务参数")){//判断是权限管理
					//通过用户和父功能查询每个子功能
					List<Right> folderList=rightService.selectSysRightByFloder(user.getUserName(), right.getRightCode());
					//将功能添加到集合中
					rightMap.put(right, folderList);
				}

			}
			//将集合添加到回话中
			session.setAttribute("rightMap", rightMap);
			//跳转到主界面并将功能集合传到页面
			return new ModelAndView("main","rightMap",rightMap);

		}else{
			//查询父功能
			List<Right> documenRighttList=rightService.selectSysRightByDocument(user.getUserName());
			//创建有序集合
			HashMap<Right,List<Right>> rightMap = new LinkedHashMap<Right, List<Right>>();;
			//循环父功能集合
			for (int i = 0; i < documenRighttList.size(); i++) {
				//通过用户和父功能查询每个子功能
				Right right = documenRighttList.get(i);
				//通过用户和父功能查询每个子功能
				List<Right> folderList=rightService.selectSysRightByFloder(user.getUserName(), right.getRightCode());
				//将功能添加到集合中
				rightMap.put(right, folderList);
			}
			//将集合添加到回话中
			session.setAttribute("rightMap", rightMap);
			//跳转到主界面并将功能集合传到页面
			return new ModelAndView("main","rightMap",rightMap);



		}

	}
	/**
	 * 通过角色查询该角色所有的权限
	 * @param role 角色
	 * @return JSON对象
	 * @throws Exception
	 */
	@ResponseBody
	@RequestMapping(value="/loadAuthorityByRole.action",produces="text/html;charset=utf-8")
	public String loadAuthorityByRole(HttpServletRequest request) throws Exception{
		//得到会话
		HttpSession session=request.getSession(false);
		if(session==null){
			session=request.getSession(true);
		}
		//得到角色对象
		Role role = (Role)session.getAttribute("role");

		//得到父功能模块的集合
		List<Right> documenRightList=rightService.selectRightByRoleAndDocument(role.getRoleCode());
		//创建map记录权限
		List<HashMap<String, Object>> allList = new ArrayList<HashMap<String, Object>>();
		//循环父功能模块
		for (int i = 0; i < documenRightList.size(); i++) {
			//创建map记录父功能的模块
			HashMap<String, Object> sysRightMap  = new HashMap<String, Object>();
			//得到每一个父功能  并添加到map集合中 用于客服端的读取
			Right right = documenRightList.get(i);
			sysRightMap.put("rightCode", right.getRightCode());
			//判断是否有该权限  并赋值给选中标志  用于客户端显示
			sysRightMap.put("checked",!right.getCheckFlag().equals("0"));
			//得到功能名
			sysRightMap.put("rightText", right.getRightText());
			//是否拥有该功能的标志
			sysRightMap.put("checkFlag", right.getCheckFlag());
			//增加标志
			sysRightMap.put("insertFlag", right.getInsertFlag());
			//修改标志
			sysRightMap.put("updateFlag", right.getUpdateFlag());
			//删除标志
			sysRightMap.put("deleteFlag", right.getDeleteFlag());
			//通过父功能编号 查询该功能下的所有的子功能
			List<Right> floderRightList=rightService.selectRightRoleAndFloder(role.getRoleCode(), right.getRightCode());
			//循环该功能下的所有的子功能 的集合
			for (int j = 0; j < floderRightList.size(); j++) {
				//得到每个子功能
				Right sRight = floderRightList.get(j);
				//判断是否有该权限  并赋值给选中标志  用于客户端显示
				sRight.setChecked(!sRight.getCheckFlag().equals("0"));
			}
			//添加父子节点关系
			sysRightMap.put("children", floderRightList);
			//将一个父子功能关系添加到map中
			allList.add(sysRightMap);
		}
		//得到转JSON的工具对象
		Gson gson = new Gson();
		//转格式
		String  jsonList= gson.toJson(allList);
		return jsonList;
	}
	/**
	 * 查询该角色拥有的权限
	 * @param role 角色对象
	 * @param request 请求对象
	 * @return 视图对象
	 * @throws Exception 异常
	 */
	@RequestMapping(value="/getQuanxian.action")
	public ModelAndView getQuanxian(@ModelAttribute("springWeb")Role role,HttpServletRequest request) throws Exception{
		//设置编码格式
		request.setCharacterEncoding("UTF-8");
		//转码
		role.setRoleName(new String(role.getRoleName().getBytes("ISO8859-1"),"UTF-8"));
		//得到会话
		HttpSession session=request.getSession(false);
		if(session==null){
			session=request.getSession(true);
		}
		//将角色添加到会话中
		session.setAttribute("role", role);
		//重定向到权限管理页面
		return new ModelAndView("redirect:jsp/quanxian.jsp");
	}
	/**
	 * 判断权限
	 * @param inRight 功能对象
	 * @param request 请求对象
	 * @return 操作标志
	 */
	@SuppressWarnings("unchecked")
	@ResponseBody
	@RequestMapping(value="/isHaveRight.action",produces="text/html;charset=utf-8")
	public String isHaveRight(@ModelAttribute("springWeb")Right inRight,HttpServletRequest request){
		Gson gson = new Gson();
		//得到会话
		HttpSession session=request.getSession(false);
		if(session==null){
			session=request.getSession(true);
		}
		//从会话中得到所有功能的集合
		HashMap<Right,List<Right>> map=(HashMap<Right,List<Right>>)session.getAttribute("rightMap");
		//迭代
		Set<Right> set = map.keySet();
		Iterator<Right> it=set.iterator();
		while(it.hasNext()) {
			//得到集合中的每个子集合
			List<Right> rightList =map.get(it.next());
			//循环
			for (Right rig:rightList) {
				//判断与传来的功能一致的功能对象
				if(rig.getRightCode().equals(inRight.getRightCode())){
					//返回该对象
					return gson.toJson(rig);

				}
			}

		}

		return "";

	}

	/**
	 * 查询所有的功能模块方法
	 * @return String 将json对象转为字符串传出视图
	 * @throws UnsupportedEncodingException 
	 */
	@SuppressWarnings("unchecked")
	@ResponseBody
	@RequestMapping(value="/selectRight.action",produces="text/html;charset=utf-8")
	public String selectRight(@ModelAttribute()Right right) throws UnsupportedEncodingException{
		right.setRightType("Folder");//查询父功能集合
		//创建有序集合
		List<HashMap<String,Object>> list=new ArrayList<HashMap<String,Object>>();
		//调用业务逻辑处理层
		HashMap<String,Object> getMap=(HashMap<String, Object>) rightService.selectRight(right);
		功能集合
		List<Right> rightList=(List<Right>) getMap.get("cursor");
		for(Right rightEntity:rightList){ 
			//创建map集合用来存储父功能集合
			HashMap<String,Object> map=new HashMap<String,Object>();
			//创建一个功能对象
			Right subRight=new Right();
			//集合添加功能编号
			map.put("rightCode", rightEntity.getRightCode());
			//集合添加功能名称
			map.put("rightText", rightEntity.getRightText());
			//集合添加功能类型
			map.put("rightType", rightEntity.getRightType());
			//集合添加功能链接
			map.put("rightUrl", rightEntity.getRightUrl());
			//集合添加功能图标
			map.put("rightIcon", rightEntity.getRightIcon());
			
			subRight.setRightParentCode(rightEntity.getRightCode());
			subRight.setRightType("Document");//查询子功能
			subRight.setPage(right.getPage());
			subRight.setRows(right.getRows());
			//得到父功能的id 通过父功能id查询子功能
			HashMap<String,Object> getSubMap=(HashMap<String, Object>) rightService.selectRight(subRight);
			//得到集合
			List<Right> subRightList=(List<Right>)getSubMap.get("cursor");//根据父功能查询子功能
			//添加子类
			map.put("children", subRightList);
			//添加到集合中
			list.add(map);
		}
		Gson gson=new Gson();
		return gson.toJson(list);
	}

	/**
	 * 增加功能的方法
	 * @param right 封装参数
	 * @return String 是否增加成功
	 */
	@ResponseBody
	@RequestMapping(value="/insertRight.action",produces="text/html;charset=UTF-8")
	public String insertRight(@ModelAttribute() Right right){
		//判断是否为父功能
		if(right.getRightType().equals("Folder")){
			
			right.setRightParentCode("");
		}
		//调用业务逻辑层
		int rows=rightService.insertRight(right);
		if(rows>0){
			return "增加成功";
		}
		return "增加失败";
	}
