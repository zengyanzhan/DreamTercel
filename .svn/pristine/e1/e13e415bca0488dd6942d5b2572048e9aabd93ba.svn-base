	/**
	 * 删除功能的方法
	 * @param rightCode 功能id
	 * @return String 是否删除成功标志
	 */
	@ResponseBody
	@RequestMapping(value="/deleteRight.action",produces="text/html;charset=UTF-8")
	public String deleteRight(@ModelAttribute() Right right){
		//得到界面上传过来的功能编号
		 String rightCode=right.getRightCode();
		 //以逗号切割为数组
		 String[] rightArray=rightCode.split(",");
		 String rightCodes=null;//用于接收删除的功能编号
		 for(int i=0;i<rightArray.length;i++){
			 //通过编号查询子功能的集合
			 List<Right> list=rightService.selectRightParentCodeByType(rightArray[i]);
			 if(list.size()>0){//如果查出来有数据 就说明该里面有父功能 需要删除该父功能下的所有子功能
				 
				 rightCodes=""+rightArray[i]+",";//得到该父功能编号
				 
				 for(int j=0;j<list.size();j++){ //遍历该集合 得到子功能编号
					 Right rightEntity=(Right)list.get(j);//得到每个实体类
					 rightCodes+="'"+rightEntity.getRightCode()+"',"; //拼接子功能编号
				 }
			 }
		 }
		 if(rightCodes==null){ //如果rightCodes为空说明 不包含父功能
			 rightCodes=right.getRightCode();
		 }else{
			 //将最后一位切掉
			 rightCodes=rightCodes.substring(0, rightCodes.length()-1);
		 }
		int rows=rightService.deleteRight(rightCodes);//调用删除的方法
		if(rows>0){
			return "删除成功";
		}
		return "删除失败";
	}

	/**
	 * 查询功能编号通过功能类型
	 * @return 生成的编号
	 * @throws UnsupportedEncodingException 异常 
	 * @author ZengYanzhan
	 * 
	 */
	@ResponseBody
	@RequestMapping(value="/selectRightCodeByType.action",produces="text/html;charset=UTF-8")
	public String selectRightCodeByType(@ModelAttribute() Right right) throws UnsupportedEncodingException{
		Gson gson=new Gson();
		String str=null;
		//判断父功能编号为空
		if(right.getRightParentCode()==null || right.getRightParentCode().equals("")){
			//查询下拉表格的父功能数据
			str=gson.toJson(rightService.selectRightParentCodeByType(null));
		}else{
			//这里没有增加一位数字
			String rightName=rightService.selectRightCodeByType(right);
			//判断数字的位数  个位添加两个0变为字符串  十位数 添加一个0变为字符串
			int count=0;//父功能长度
			int num=0;//计数
			if(right.getRightParentCode().equals("one")){//如果为one标志为父功能
				count=1;
				str="FolderL";//增加前缀标志为父功能
			}else{
				count=right.getRightParentCode().length()+1;//得到父功能的编号长度
				str=right.getRightParentCode();//得到父功能的编号
			}
			if(rightName!=null && !rightName.equals("")){//如果从数据库得到的功能编号不为空就意味着不是第一个 进入+1
				num=Integer.parseInt(rightName.substring(count));//切割 并且转为int类型
				num++; //+1
				if(num<=9){ //拼接前缀
					str=str+"0"+num;
				}else if(num<=99){
					str=str+""+num;
				} 
			}else{
				str=str+"01";//如果没有数据就显示第一个
			}
			System.err.println(str);
		}
		return str;
	}

