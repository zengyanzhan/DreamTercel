public class Role {

	private  String  roleCode;//角色ID
	private  String  roleName;//角色名 
	private  String  roleDepict;//角色描述
	private  Integer roleFlag;//角色状态
	private  String  roleDesc;//备注
	private  Integer page;//当前的页数
	private  Integer rows;//当前的行数
	private  String  sortName;//要排序的列
	private  String  sortOrder;//排序的方式
	public Role(String roleCode, String roleName, String roleDepict, Integer roleFlag, String roleDesc) {
		super();
		this.roleCode = roleCode;
		this.roleName = roleName;
		this.roleDepict = roleDepict;
		this.roleFlag = roleFlag;
		this.roleDesc = roleDesc;
	}
	public Role(){}
	public Role(String roleCode, String roleName, String roleDepict, Integer roleFlag, String roleDesc, Integer page,
			Integer rows, String sortName, String sortOrder) {
		super();
		this.roleCode = roleCode;
		this.roleName = roleName;
		this.roleDepict = roleDepict;
		this.roleFlag = roleFlag;
		this.roleDesc = roleDesc;
		this.page = page;
		this.rows = rows;
		this.sortName = sortName;
		this.sortOrder = sortOrder;
	}
	public Integer getPage() {
		return page;
	}
	public void setPage(Integer page) {
		this.page = page;
	}
	public Integer getRows() {
		return rows;
	}
	public void setRows(Integer rows) {
		this.rows = rows;
	}
	public String getSortName() {
		return sortName;
	}
	public void setSortName(String sortName) {
		this.sortName = sortName;
	}
	public String getSortOrder() {
		return sortOrder;
	}
	public void setSortOrder(String sortOrder) {
		this.sortOrder = sortOrder;
	}
	public String getRoleCode() {
		return roleCode;
	}
	public void setRoleCode(String roleCode) {
		this.roleCode = roleCode;
	}
	public String getRoleName() {
		return roleName;
	}
	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}
	public String getRoleDepict() {
		return roleDepict;
	}
	public void setRoleDepict(String roleDepict) {
		this.roleDepict = roleDepict;
	}
	public Integer getRoleFlag() {
		return roleFlag;
	}
	public void setRoleFlag(Integer roleFlag) {
		this.roleFlag = roleFlag;
	}
	public String getRoleDesc() {
		return roleDesc;
	}
	public void setRoleDesc(String roleDesc) {
		this.roleDesc = roleDesc;
	}
	@Override
	public String toString() {
		return "Role [roleCode=" + roleCode + ", roleName=" + roleName + ", roleDepict=" + roleDepict + ", roleFlag="
				+ roleFlag + ", roleDesc=" + roleDesc + ", page=" + page + ", rows=" + rows + ", sortName=" + sortName
				+ ", sortOrder=" + sortOrder + "]";
	}
	


}
