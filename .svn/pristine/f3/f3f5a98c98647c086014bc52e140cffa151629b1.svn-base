package com.yidu.system.domain;

import java.io.Serializable;
import java.util.Date;
/**
 * 节假日实体类
 * @author Lee
 * @date 2017年11月14日
 * @time 下午3:14:46
 */
public class Holiday implements Serializable{
	Integer hCode;
	Date  hDate;
	public Holiday (){}
	public Holiday(Integer hCode, Date hDate) {
		super();
		this.hCode = hCode;
		this.hDate = hDate;
	}
	public Integer gethCode() {
		return hCode;
	}
	public void sethCode(Integer hCode) {
		this.hCode = hCode;
	}
	public Date gethDate() {
		return hDate;
	}
	public void sethDate(Date hDate) {
		this.hDate = hDate;
	}
	@Override
	public String toString() {
		return "Holiday [hCode=" + hCode + ", hDate=" + hDate + "]";
	}
	
}
