* {
	margin: 0;
	padding: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

body {
	background: #f8f8f8;
	font-family: 'Open Sans', Arial, Helvetica, Sans-serif, Verdana, Tahoma;
}

ul {
	list-style-type: none;
}

a {
	color: #b63b4d;
	text-decoration: none;
}

/** =======================
 * Contenedor Principal
 ===========================*/
h1 {
 	color: #FFF;
 	font-size: 20px;
 	font-weight: 100%;
 	text-align: center;
 	margin-top: 80px;
 }

h1 a {
 	color: #c12c42;
 	font-size: 16px;
 }

 .accordion {
position: absolute;
 	width: 100%;
 	max-width: 100%;
 	margin-left: -12px;
	/*手风琴的初始背景颜色*/
 	background: #dcdcdc;
 	-webkit-border-radius: 4px;
 	-moz-border-radius: 4px;
 	border-radius: 4px;
 }

.accordion .link {
	cursor: pointer;
	display: block;
	padding: 15px 15px 15px 42px;
	/*手风琴初始的项颜色*/
	color: #4D4D4D;
	/*手风琴字体大小*/
	font-size: 12px;
	font-weight: 100%;
	border-bottom: 1px solid #CCC;
	position: relative;
	height:40px;
	-webkit-transition: all 0.4s ease;
	-o-transition: all 0.4s ease;
	transition: all 0.4s ease;
}

.accordion li:last-child .link {
	border-bottom: 0;
}

.accordion li i {
	position: absolute;
	top: 16px;
	left: 12px;
	font-size: 10px;
	/*手风琴初始的图标颜色*/
	color: #595959;
	-webkit-transition: all 0.4s ease;
	-o-transition: all 0.4s ease;
	transition: all 0.4s ease;
}

.accordion li i.fa-chevron-down {
	right: 12px;
	left: auto;
	font-size: 10px;
}

.accordion li.open .link {
	/*手风琴展开的项的字体变的颜色*/
	color: red;
}

.accordion li.open i {
	/*手风琴展开的项的图标变的颜色*/
	color: #b63b4d;
}
.accordion li.open i.fa-chevron-down {
	-webkit-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	-o-transform: rotate(180deg);
	transform: rotate(180deg);
}

/**
 * Submenu
 -----------------------------*/
 .submenu {
 	display: none;
	/*手风琴展开的子项的背景颜色*/
 	background: #444359;
 	font-size: 10px;
 }

 .submenu li {
	/*手风琴展开的子项的分割线的颜色*/
 	border-bottom: 1px solid #4b4a5e;
 }

 .submenu a {
 	display: block;
 	text-decoration: none;
	/*手风琴展开的子项的的字体的颜色*/
 	color: #d9d9d9;
 	padding: 12px;
 	padding-left: 42px;
 	-webkit-transition: all 0.25s ease;
 	-o-transition: all 0.25s ease;
 	transition: all 0.25s ease;
 }

 .submenu a:hover {
	/*手风琴展开的子项的鼠标移上去的背景颜色*/
 	background: #b63b4d;
	/*手风琴展开的子项的鼠标移上去的字体颜色*/
 	color: #FFF;
 }

	
